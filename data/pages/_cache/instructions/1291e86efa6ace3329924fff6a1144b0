pcm.my_card { 
  type hw
  card 0
# emulation nmap, a décommenter si une application se plaint de nmap
# mmap_emulation true
}

pcm.dmixed {
  type dmix 
  ipc_key 1024 
  slave {
   pcm "my_card" 
   period_time 0
   period_size 1024
   buffer_size 4096
   periods 128
   rate 48000 
     }
}

pcm.dsnooped {
  type dsnoop 
  ipc_key 2048 
  slave {
    pcm "my_card" 
    rate 48000
    period_size 1024
  }
}

pcm.asymed {
  type asym 
  playback.pcm "dmixed" 
  capture.pcm "dsnooped"
}

pcm.pasymed {
  type plug 
  slave.pcm "asymed"
}

ctl.pasymed { # un controleur pour xmms entre autre
        type hw
        card 0
}

pcm.dsp0 { # Pour les applis lancé par aoss monappli
  type plug
  slave.pcm "asymed"
}

pcm.!default { # La sortie par defaut
  type plug
  slave.pcm "asymed"
}
ctl.mixer0 {
        type hw
        card 0
}
