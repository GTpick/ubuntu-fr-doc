a:2:{s:7:"current";a:7:{s:5:"title";s:7:"Quanta+";s:11:"description";a:2:{s:8:"abstract";s:485:"Quanta+ est un environnement de développement Web qui prend en charge la plupart des langages utilisés sur le Web (XHTML, PHP, Javascript, CSS). Il supporte aussi la plupart des encodages (il est conseillé d'utiliser l'UTF-8, qui est le standard d'Ubuntu). Quanta+ supporte l'auto-complétion des variables et des fonctions dans la plupart des langages supportés. Ce programme utilise les bibliothèques graphiques de KDE, ne soyez donc pas surpris de voir QT s'installer avec lui.";s:15:"tableofcontents";a:5:{i:0;a:4:{s:3:"hid";s:14:"l_installation";s:5:"title";s:14:"L'installation";s:4:"type";s:2:"ul";s:5:"level";i:1;}i:1;a:4:{s:3:"hid";s:22:"francisation_de_quanta";s:5:"title";s:23:"Francisation de Quanta+";s:4:"type";s:2:"ul";s:5:"level";i:1;}i:2;a:4:{s:3:"hid";s:20:"ouvrir_les_documents";s:5:"title";s:20:"Ouvrir les documents";s:4:"type";s:2:"ul";s:5:"level";i:2;}i:3;a:4:{s:3:"hid";s:19:"quanta_gnome_et_ssh";s:5:"title";s:20:"Quanta, Gnome et SSH";s:4:"type";s:2:"ul";s:5:"level";i:1;}i:4;a:4:{s:3:"hid";s:13:"remerciements";s:5:"title";s:13:"Remerciements";s:4:"type";s:2:"ul";s:5:"level";i:1;}}}s:8:"relation";a:2:{s:7:"haspart";a:0:{}s:10:"references";a:1:{s:36:"tutoriel:comment_installer_un_paquet";b:1;}}s:4:"date";a:3:{s:5:"valid";a:0:{}s:7:"created";i:1177578185;s:8:"modified";i:1195910456;}s:7:"creator";s:8:"cipher16";s:11:"last_change";b:0;s:11:"contributor";a:5:{i:0;N;s:12:"fxdarkplayer";s:12:"fxdarkplayer";s:5:"bruno";s:5:"bruno";s:13:"fidji-florian";s:13:"fidji-florian";s:6:"Pirhoo";s:6:"Pirhoo";}}s:10:"persistent";a:4:{s:4:"date";a:2:{s:7:"created";i:1177578185;s:8:"modified";i:1195910456;}s:7:"creator";s:8:"cipher16";s:11:"last_change";b:0;s:11:"contributor";a:5:{i:0;N;s:12:"fxdarkplayer";s:12:"fxdarkplayer";s:5:"bruno";s:5:"bruno";s:13:"fidji-florian";s:13:"fidji-florian";s:6:"Pirhoo";s:6:"Pirhoo";}}}